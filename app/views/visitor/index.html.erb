<p id="notice"><%= notice %></p>

<h1> Ideas Showcase</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Skills</th>
      <th>Additional</th>
   
    </tr>
  </thead>

  <tbody>
  <%@ideas = Idea.all%>
    <% @ideas.each do |idea| %>
      <tr>
        <td><%= idea.title %></td>
        <td><%= idea.description %></td>
        <td><%= idea.skills %></td>
        <td><%= idea.additional %></td>
      
      </tr>
    <% end %>
  </tbody>
</table>

<br>





  </div>
